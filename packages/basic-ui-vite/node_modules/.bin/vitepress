#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yinzhuo/Desktop/studyProject/basic-ui-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yinzhuo/Desktop/studyProject/basic-ui-vite/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/registry.npmmirror.com+vitepress@0.22.4/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/registry.npmmirror.com+vitepress@0.22.4/node_modules/vitepress/bin/vitepress.js" "$@"
fi
